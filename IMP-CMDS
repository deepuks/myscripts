# strace -Tttvo /tmp/perl.st -s 4096 /usr/bin/perl /usr/bin/cpan
for i in `find /usr/lib64/ -name "*.so"`; do ret=`objdump -p "$i" |grep NEEDED | grep "libgtk."`; if [ -n "$ret" ]; then echo "$i: $ret"; fi ; done

